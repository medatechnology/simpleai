# SimpleAI API Environment Variables
# Copy this file to .env and fill in your values

# Required: Mistral API Key (primary provider)
MISTRAL_API_KEY=your-mistral-api-key

# Optional: Mistral model (defaults to mistral-large-latest)
MISTRAL_MODEL=mistral-large-latest

# Optional: OpenAI API Key (fallback provider)
OPENAI_API_KEY=

# Server port
SIMPLEHTTP_PORT=8080
